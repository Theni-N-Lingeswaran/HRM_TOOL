<div id="<%= dom_id task %>">
  <p>
    <strong>Title:</strong>
    <%= task.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>Complete:</strong>
    <%= task.complete %>
  </p>

  <p>
    <strong>Delist:</strong>
    <%= task.delist %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= task.token %>
  </p>

</div>
